    "bootstrap": "^4.5.3",
    "clean-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "4.4.1",
    "immer": "^7.0.9",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
