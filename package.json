  },
  "scripts": {
    "build": "webpack -p",
    "start": "webpack-dev-server -d --content-base ./public",
    "deploy": "yarn install && yarn build",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "extract": "formatjs extract 'src/**/*.ts*' --out-file src/lang/tw.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "compile": "formatjs compile src/lang/tw.json --ast --out-file src/lang/compiled/tw.json"
  },
  "resolutions": {
    "styled-components": "^5"
  },
